{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/auth/(.*)", "dest": "/index.js" },
        { "src": "/friend/(.*)", "dest": "/index.js" },
        { "src": "/post/(.*)", "dest": "/index.js" },
        { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  